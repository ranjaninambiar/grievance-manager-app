<!DOCTYPE html>
<html>
<head>
    
    <title><%= title %></title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstraps/4.3.1/js/bootstrap.min.js:"></script>
</head>
<body>
<a href="emp_view"><Button>Go Back</Button></a>

<div class="container">
    <h2><%= title %></h2>
    <table class="table table-dark table-stripped">
        <thead>
            <tr>
                <th>Grievance_id</th>
                <th>Status</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach(function(user){%>
                <tr>
                    <td><%= user.grievance_id %></td>
                    <td><%= user.status %></td>
                    <td>
                        <a href="edit/<%= user.grievance_id %>" class="btn btn-sm btn-primary">Edit</a>
                    </td>
                </tr>
            <%}); %>
        </tbody>
    </table>
</div>


</body>
</html>
